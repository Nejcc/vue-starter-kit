{
    "name": "laravelplus/ecommerce",
    "description": "An ecommerce package for Laravel with products, categories, variants, and admin panel support.",
    "keywords": ["laravel", "ecommerce", "shop", "products", "categories"],
    "license": "MIT",
    "authors": [
        {
            "name": "LaravelPlus",
            "email": "info@laravelplus.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/http": "^12.0"
    },
    "require-dev": {
        "inertiajs/inertia-laravel": "^2.0",
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-permission": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelPlus\\Ecommerce\\": "src/",
            "LaravelPlus\\Ecommerce\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelPlus\\Ecommerce\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelPlus\\Ecommerce\\EcommerceServiceProvider"
            ],
            "aliases": {
                "Ecommerce": "LaravelPlus\\Ecommerce\\Facades\\Ecommerce"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
